{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-yumiserver",
    "stop": "egg-scripts stop --title=egg-server-yumiserver",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "m:create": "sequelize migration:create",
    "m:up": "sequelize migration:up",
    "m:down": "sequelize migration:undo",
    "model:generate": "./node_modules/.bin/sequelize model:generate"
  },
  "dependencies": {
    "egg": "^2.6.1",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^5.2.1",
    "mysql2": "^2.1.0",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^7.10.9",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.8.0",
    "eslint-config-egg": "^8.0.0",
    "tslib": "^1.11.1",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "Samuel <wushuang1227@gmail.com>",
  "license": "MIT"
}
